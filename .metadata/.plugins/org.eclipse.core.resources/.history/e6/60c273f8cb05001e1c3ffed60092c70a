
public class TipoVariable {

	public static void main(String[] args) {
		System.out.println("! Hola Mundo Java Oracle One ยก");
		int edad = 18;
		System.out.println(edad);

		edad = 28;
		System.out.println(edad);

		int edad2 = 38;
		System.out.println(edad2);

		int edad3 = 48;
		System.out.println(edad3);
		
		int totalEdades = edad + edad2 + edad3;
		
		

		System.out.println("Total de  sumatoria de todas las edades : " + totalEdades);
	}

}
