import java.util.Arrays;

public class Conversion_variables {
	public static void main(String[] args) {

		double variable1 = 230.89;
		int variable2 = (int) variable1; //castiando(cats) ==> << int variable2 = (int) variable1 >> Obligando a variable1 a ser de tipo entero
		var str = Arrays.toString(args);
		//System.out.println("variable2 = (int) variable1 => " + variable2);
		System.out.println(str);

	}

}
